		datedialog.h \
		spreadsheet.h \
		dateinterval.h \
		calc.h

datedialog.o: datedialog.cpp datedialog.h

cell.o: cell.cpp cell.h

spreadsheet.o: spreadsheet.cpp cell.h \
		spreadsheet.h

dateinterval.o: dateinterval.cpp dateinterval.h

moc_mainform.o: moc_mainform.cpp  mainform.h 

moc_datedialog.o: moc_datedialog.cpp  datedialog.h 

moc_spreadsheet.o: moc_spreadsheet.cpp  spreadsheet.h 

moc_dateinterval.o: moc_dateinterval.cpp  dateinterval.h 

moc_mainform.cpp: $(MOC) mainform.h
	$(MOC) mainform.h -o moc_mainform.cpp

moc_datedialog.cpp: $(MOC) datedialog.h
	$(MOC) datedialog.h -o moc_datedialog.cpp

moc_spreadsheet.cpp: $(MOC) spreadsheet.h
	$(MOC) spreadsheet.h -o moc_spreadsheet.cpp

moc_dateinterval.cpp: $(MOC) dateinterval.h
	$(MOC) dateinterval.h -o moc_dateinterval.cpp

qmake_image_collection.o: qmake_image_collection.cpp

qmake_image_collection.cpp: icons/editdelete.png \
		icons/fileclose.png \
		icons/filenew.png \
